{
    "name": "assimtech/tempo",
    "type": "library",
    "description": "A scripting tool for running commands on local or remote nodes",
    "homepage": "https://github.com/assimtech/tempo",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Assimtech\\Tempo\\": "src"
        }
    },
    "require": {
        "php": ">=5.3.3",

        "symfony/console": "~2.0",
        "symfony/process": "~2.0",
        "symfony/yaml": "~2.0"
    },
    "require-dev": {
        "sebastian/phpcpd": "~2.0",
        "squizlabs/php_codesniffer": "~2.3",
        "phpmd/phpmd": "~2.2",
        "phpspec/phpspec": "~2.2",
        "henrikbjorn/phpspec-code-coverage": "~1.0"
    },
    "bin": [ "bin/tempo" ]
}
